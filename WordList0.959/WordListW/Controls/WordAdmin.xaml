            </StackPanel>
            <StackPanel Name="stkSynonym" Orientation="Horizontal">

            </StackPanel>
            <StackPanel Name="stkAntonym" Orientation="Horizontal">

            </StackPanel>
            <StackPanel Name="stkHomonym" Orientation="Horizontal">

            </StackPanel>
            <StackPanel Name="stkWordUsage" Orientation="Horizontal">

            </StackPanel>
            <StackPanel Name="stkOther" Orientation="Horizontal">

            </StackPanel>
        </StackPanel>

    </Grid>
</UserControl>
